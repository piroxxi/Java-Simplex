<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g="urn:import:com.google.gwt.user.client.ui">


	<ui:style>
		.button {
			background: none repeat scroll 0 0 #BF9764;
			border: medium none;
			border-radius: 7px 7px 7px 7px;
			color: #3A1F05;
			font-size: 15px;
			height: 22px;
			margin-right: 15px;
		}
	</ui:style>

	<g:HorizontalPanel>
		<g:Button ui:field="main" styleName="{style.button}">welcome</g:Button>
		<g:Button ui:field="exemple" styleName="{style.button}">GOTO_EXAMPLE</g:Button>
	</g:HorizontalPanel>
</ui:UiBinder>