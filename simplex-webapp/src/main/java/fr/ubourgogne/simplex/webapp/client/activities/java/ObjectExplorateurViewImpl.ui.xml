<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g="urn:import:com.google.gwt.user.client.ui">

	<ui:style>
		.arrows {
			position: relative;
			height: 0px;
			width: 0px;
		}
		
		.panel {
			width: 700px;
		}
	</ui:style>

	<g:HTMLPanel>
		<g:HTMLPanel ui:field="arrowPanel" styleName="{style.arrows}"></g:HTMLPanel>
		<g:SimplePanel ui:field="panel" styleName="{style.panel}"></g:SimplePanel>
	</g:HTMLPanel>
</ui:UiBinder>